@model IEnumerable<Inter_New_App_Joins.DbConnection.tbl_Student>

@{
    ViewBag.Title = "Index";
}

<div class=" text-center">
    <h2 class="text-primary">Student's Details</h2>
</div>



<table class="table table-bordered">
    <tr>
        <th>
            @Html.DisplayNameFor(model => model.Name)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Email)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Standard)
        </th>
        <th>
            @Html.DisplayNameFor(model => model.Address)
        </th>
        <th></th>
    </tr>

@foreach (var item in Model) {
    <tr>
        <td>
            @Html.DisplayFor(modelItem => item.Name)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Email)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Standard)
        </td>
        <td>
            @Html.DisplayFor(modelItem => item.Address)
        </td>
        <td>
            @Html.ActionLink("Edit", "Edit", new { id=item.Stu_Id }) |
          
            @Html.ActionLink("Delete", "DeleteStudent", new { id=item.Stu_Id })
        </td>
    </tr>
}

</table>
